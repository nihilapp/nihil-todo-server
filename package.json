{
  "name": "nihil-nest-template",
  "version": "1.0.0",
  "description": "Nest 편하게 하려고 만든 템플릿.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nihil-template/nihil-nest-template.git"
  },
  "bugs": {
    "url": "https://github.com/nihil-template/nihil-nest-template/issues"
  },
  "homepage": "https://github.com/nihil-template/nihil-nest-template#readme",
  "scripts": {
    "build": "yarn run p:migrate && nest build",
    "n:dev": "yarn run p:migrate && nest start --watch",
    "n:start": "yarn run p:migrate && nest start",
    "p:init": "npx prisma init",
    "p:create": "npx prisma migrate dev --create-only --name update",
    "p:migrate": "npx prisma migrate dev  --name update",
    "p:push": "npx prisma db push",
    "p:pull": "npx prisma db pull",
    "p:studio": "npx prisma studio"
  },
